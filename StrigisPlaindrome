
import java.util.*;
class Main {
    public static void main(String[] args) {
        String s="MOM";
        char arr[]=s.toCharArray();
        int n=arr.length;
        String res="";
        for(int i=n-1;i>=0;i--)
        {
            
            res=res+arr[i];
        }
       
        if(s.equals(res))
        {
            System.out.println("Is palindrome");
        }
        else
        {
        System.out.println("Is Not palindrome");
        }
        
    }
}
