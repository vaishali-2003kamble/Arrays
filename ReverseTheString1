

class Main {
    public static void main(String[] args) {
        String s="vaishali";
        char arr[]=s.toCharArray();
        int n=arr.length;
        String res="";
        for(int i=n-1;i>=0;i--)
        {
            
            res=res+arr[i];
        }
        System.out.println("Reverse :- "+res);
        
        
    }
}
