
public class Main {
    public static void main(String[] args) {
        
          String s="Vaishali";
          int low=0;
          int high=s.length()-1;
          char c[]=s.toCharArray();
          while(low<high)
          {
              char temp=c[low];
              c[low++]=c[high];
              c[high--]=temp;
          }
          String str=new String(c);
          System.out.println("Rever the string :- "+str);
          
       
    }
}
