
public class Main {
    public int fact(int n)
    {
        if(n==0 || n==1)
        {
            return 1;
        }
        return n*fact(n-1);
    }
    public static void main(String[] args) {
        Main m=new Main();
        int n=5;
        int p= m.fact(n);
         System.out.println("Factorial of the number :- "+p);
        
       
    }
}
