

public class RemoveDuplicateInString {

	public static void main(String[] args) {
	
		String s="Vaishali";
		char arr[]=s.toCharArray();
		String res="";
		int n=arr.length;
		for(int i=0;i<n;i++ )
		{
			boolean dup=false;
			
			for(int j=0;j<res.length();j++)
			{
				if(arr[i]==s.charAt(j))
				{
					dup=true;
					break;
				}
				
			}
			
			if(!dup)
			{
				res=res+arr[i];
			}
		}
		
		
			System.out.println("Unique Element are :- "+res);
		
		
	}

}
